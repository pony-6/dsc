{
    "name": "dscmall/laravel-scout-elastic",
    "description": "Elastic Driver for Laravel Scout",
    "keywords": ["laravel", "scout", "elasticsearch", "elastic"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "laravel/scout": "^7.2",
        "elasticsearch/elasticsearch": "^7.5"
    },
     "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "ScoutEngines\\Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoutEngines\\Elasticsearch\\Test\\": "tests"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^7.5)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScoutEngines\\Elasticsearch\\ElasticsearchProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
